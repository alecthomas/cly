<?xml version="1.0"?>
<grammar>
  <node name="quit" help="Quit">
    <action callback="do_quit()" help="Quit"/>
  </node>
  <node name="cat" help="Concatenate files">
    <variable name="file" help="File to concatenate" pattern="\S+">
      <action callback="do_cat(file)" help="Concatenate files"/>
    </variable>
  </node>
</grammar>
